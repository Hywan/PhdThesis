digraph {
    graph [ratio=compress, splines=line];
    node [shape=plaintext, height=.3];
    compound=true;

    ac  -> sdc;
    ac  -> spc;
    sdc -> dc;
    sdc -> sc;
    spc -> sc;
    spc -> pc;

    ac  [width=1.0, label=" ", texlbl="Couverture \inenglish{All}-$G$"];
    sc  [width=1.0, label=" ", texlbl="Couverture Clause"];
    dc  [width=1.0, label=" ", texlbl="Couverture Domaine"];
    pc  [width=1.0, label=" ", texlbl="Couverture Prédicat"];
    sdc [width=1.0, label=" ", texlbl="\begin{tabular}{c}Couverture \\ Clause~+~ Domaine\end{tabular}"];
    spc [width=1.0, label=" ", texlbl="\begin{tabular}{c}Couverture \\ Clause~+~Prédicat\end{tabular}"];
}
